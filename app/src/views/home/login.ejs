<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
        <link rel="stylesheet" href="/css/home/login.css">
        <script src="/js/home/login.js" defer></script>
        <title>Login</title>
    </head>
    <body>
        <ul class="notifications"></ul>
        <!-- dummy -->
        <input type="text" style="display:none" aria-hidden="true">
        <input type="password" style="display:none" aria-hidden="true">
        <!-- main -->
        <div class="box">
            <h2>Login</h2>
            <div class="input">
                <input type="text" id="id" required="" autocomplete="off">
                <label>Id</label>
            </div>
            <div class="input">
                <input type="password" id="pswd" required="" autocomplete="new-password">
                <label>Password</label>
            </div>
            <div class="btn-box">
                <button id="login-btn">
                    <a id="btn">
                        <span></span>
                        <span></span>
                        <span></span>
                        <span></span>
                        Login
                    </a>
                </button>
            </div>
            <div class="btn-box2">
                <button id="signup-btn">
                    <a id="btn2" href="/signup">
                        <span></span>
                        <span></span>
                        <span></span>
                        <span></span>
                        Sign up
                    </a>
                </button>
            </div>
          </div>
    </body>
</html> 